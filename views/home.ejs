<script src="https://unpkg.com/scrollreveal"></script>
<script src="https://cdn.jsdeliver.net/npm/typed.js@2.0.12"></script>

<script>
    
    
    
    let section = document.querySelectorAll('section');
    let navLinks = document.querySelectorAll('header nav a');


    $(document).ready(function(){
        $('.filter-item').click(function(){
            const value = $(this).attr('data-filter')
            if(value == 'all') {
                $('.post-box').show('1000')
            }
            else {
                $('.post-box').not('.' + value).hide('1000')
                $('.post-box').filter('.' + value).show('1000')
            }
        });
        $('.filter-item').click(function(){
            $(this).addClass("active-filter").siblings().removeClass("active-filter");
        });
    })
    
    const header = document.querySelector('header')
    
    window.addEventListener('scroll', () => {
        header.classList.toggle('shadow', window.scrollY > 0);
    })
  








    

        window.onscroll = () => {
            section.forEach(sec => {
                let top = window.scrollY;
                let offset = sec.offsetTop - 150;
                let height = sec.offsetHeight;
                let id =sec.getAttribute('id')
                
                if(top >= offset && top < offset + height) {
                    navLink.forEach(links => {
                        links.classList.remove('active');
                        document.querySelector('header nav a [href*=' + id +']').classList.add('active');
                        
                    });

                }
            });
        }

        let headerr = document.querySelector('header');

        header.classList.toggle('sticky', window.scrollY > 100);
        ScrollReveal ({
            //reset:true,
            distance: '100px',
            duration: 2000,
            delay: 200,
        });

        ScrollReveal().reveal('.home-text, .About-header', {origin:'top'});
        ScrollReveal().reveal('.h2, .one, .media-coverage',{origin:'bottom'});
        ScrollReveal().reveal('.home-subtitle, .two' ,{origin:'right'});
        ScrollReveal().reveal('.div .one',{origin:'left'});





        const typed = new Typed ('.multiple-text', {
            strings: ['web designer', 'frontend engineer', 'computer engineer'],
            typeSpeed: 100,
            backSpeed : 100,
            backDelay: 1000,
            loop: true
        })






        let json_url = "cvblog.json";

    
        fetch(json_url).then((Response) = Response.json())
        .then(data => {
             let documents= data.filter(ele => {
                return ele.name==="CV";
        });
        document.getElementById('title').innerText = documents [0].name;
        document.getElementById('waliyu1.docx').href = documents [0].low;
        document.getElementById('gen').innerText = documents [0].genre;
        document.getElementById('date').innerText = documents [0].date;
        document.getElementById('rate').innerHTML = documents [0].name;



    })







        

        
    

</script>
